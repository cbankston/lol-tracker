<h2>Summoners</h2>

<div class="row">
  {{~it.summoners :summoner:index}}
    <div class="summoner-thumbnail">
      <a href="/summoners/{{=summoner._id}}/champions">
        <div class="well thumbnail well-thumbnail">
          <img src="{{=summoner.image_path()}}" />
          <div class="caption">
            <span class="h5 summoner-name">{{=summoner.name}}</span><br />
            <span class="h6 summoner-level">Level {{=summoner.summonerLevel}}</span>
          </div>
        </div>
      </a>
    </div>
  {{~}}
</div>
